version: "3.7"
services:
  mqtt-broker:
    image: eclipse-mosquitto
    hostname: mqtt-broker
    ports:
      - "1883:1883"
      - "9001:9001"
    networks:
        - fiware_backend

    volumes:
      - ./mosquitto.conf:/mosquitto/config/mosquitto.conf

networks:
  fiware_backend:
     external: true
